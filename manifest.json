{
  "manifest_version": 2,
  "name": "PhoneNumberPosting",
  "short_name": "PhoneNumberPosting",
  "description": "LinkedIn automation extension to find leads and engage with prospects.",
  "version": "1.0.5",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "storage",
    "http://app.dataway.nl/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png"
    },
    "default_title": "PhoneNumberPosting",
    "default_popup": "html/popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.js",
        "js/contents.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "js/*.js",
    "styles/*.css",
    "images/*.png"
  ]
}
